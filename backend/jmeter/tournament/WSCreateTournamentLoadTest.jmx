            <stringProp name="Argument.metadata">=</stringProp>
          </elementProp>
          <elementProp name="executionId" elementType="Argument">
            <stringProp name="Argument.name">executionId</stringProp>
            <stringProp name="Argument.value">11</stringProp>
            <stringProp name="Argument.metadata">=</stringProp>
